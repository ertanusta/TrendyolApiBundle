{
  "name": "ertanusta/trendyol-api-bundle",
  "description": "Trendyol API servislerini kullanmayı sağlayan bundle",
  "type": "symfony-bundle",
  "version": "1.0",
  "keywords": ["Symfony", "bundle", "trendyol", "api"],
  "require": {
    "php": ">=7.1",
    "symfony/event-dispatcher": "^4.4|^5.3|^6.0",
    "symfony/http-client": "^4.4|^5.3",
    "symfony/validator": "^4.4|^5.3",
    "symfony/http-foundation": ">=v3.4.47",
    "symfony/framework-bundle": "^4.4|^5.3",
    "symfony/serializer": "^5.3"
  },
  "require-dev": {
    "symfony/serializer":           "^4.3|^5.3",
    "phpunit/phpunit":              "^7.5|^8.5",
    "phpstan/phpstan":              "^0.12.11",
    "phpstan/phpstan-phpunit":      "^0.12.6"
  },
  "autoload": {
    "psr-4": {
      "Trendyol\\ApiBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Trendyol\\ApiBundle\\Tests\\": "Tests/"
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Ertan Usta",
      "email": "ertanusta@outlook.com"
    }
  ]
}
